{
	"name": "neuro-hub",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint . --ext .ts,.tsx",
		"lint:ci": "eslint . --ext .ts,.tsx --max-warnings 0",
		"lint:fix": "eslint . --ext .ts,.tsx --fix",
		"lint-staged": "lint-staged",
		"format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css}\"",
		"format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css}\"",
		"type-check": "tsc --noEmit",
		"test": "jest --passWithNoTests",
		"test:watch": "jest --watch --passWithNoTests",
		"db:migrate": "tsx node_modules/.bin/knex migrate:latest --knexfile knexfile.ts",
		"db:migrate:make": "tsx node_modules/.bin/knex migrate:make --knexfile knexfile.ts",
		"db:migrate:rollback": "tsx node_modules/.bin/knex migrate:rollback --knexfile knexfile.ts",
		"db:migrate:status": "tsx node_modules/.bin/knex migrate:status --knexfile knexfile.ts",
		"db:seed": "tsx node_modules/.bin/knex seed:run --knexfile knexfile.ts",
		"db:seed:make": "tsx node_modules/.bin/knex seed:make --knexfile knexfile.ts",
		"db:test": "tsx src/lib/db/test-connection.ts",
		"prepare": "husky"
	},
	"dependencies": {
		"@emotion/cache": "^11.13.5",
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@mui/material": "^7.3.2",
		"@mui/material-nextjs": "^7.3.2",
		"better-auth": "^1.4.16",
		"effector": "^23.3.0",
		"effector-react": "^23.3.0",
		"immer": "^11.1.3",
		"knex": "3.1.0",
		"next": "^16.1.4",
		"next-swagger-doc": "^0.4.0",
		"pg": "^8.14.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"swagger-ui-react": "^5.18.2",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@jest/test-sequencer": "^30.2.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.2",
		"@types/jest": "^30.0.0",
		"@types/node": "^25.0.9",
		"@types/pg": "^8.11.11",
		"@types/react": "^19.0.6",
		"@types/react-dom": "^19.0.3",
		"@typescript-eslint/eslint-plugin": "^8.20.0",
		"@typescript-eslint/parser": "^8.20.0",
		"eslint": "^9.18.0",
		"eslint-config-next": "^16.1.4",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-react": "^7.37.3",
		"eslint-plugin-react-hooks": "^7.0.1",
		"husky": "^9.1.7",
		"jest": "^30.2.0",
		"jest-environment-jsdom": "^30.2.0",
		"lint-staged": "^16.2.7",
		"prettier": "^3.4.2",
		"tsx": "^4.21.0",
		"typescript": "^5.9.0"
	},
	"engines": {
		"node": ">=24.0.0",
		"yarn": ">=1.0.0"
	},
	"packageManager": "yarn@1.22.22"
}
